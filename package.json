{
  "name": "servico-autenticacao",
  "version": "0.0.1",
  "description": "",
  "author": "Pedro Marcelo <pedromarcelodesaalves@gmail.com>",
  "repository": "git@github.com:open-beauty-ce/servico-autenticacao.git",
  "private": true,
  "license": "MIT",
  "scripts": {
    "docker": "./docker/bin/run-docker.sh",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:grpc": "npm rebuild grpc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build:grpc && cross-env NODE_ENV=development nest start",
    "start:dev": "npm run build:grpc && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "npm run build:grpc && cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "npm run build:grpc && cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run build:grpc && cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "npm run build:grpc && cross-env NODE_ENV=test jest --watch --runInBand",
    "test:cov": "npm run build:grpc && cross-env NODE_ENV=test jest --coverage --runInBand",
    "test:debug": "npm run build:grpc && cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run build:grpc && cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/microservices": "^7.0.7",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/platform-express": "^7.0.0",
    "@types/mongodb": "^3.5.5",
    "@types/mongoose-autopopulate": "^0.10.0",
    "@types/mongoose-paginate": "^5.0.8",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.2",
    "descricao-servicos": "git+ssh://git@github.com:open-beauty-ce/definicao-servicos.git",
    "grpc": "^1.24.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.9",
    "mongoose-autopopulate": "^0.12.1",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.7.11",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "mongodb-memory-server": "^6.5.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
